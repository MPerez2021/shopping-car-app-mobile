{
  "expo": {
    "name": "Go Market",
    "slug": "go_market",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icon22.png",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splashScreen1.png",
      "resizeMode": "contain",
      "backgroundColor": "#231e1c"
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "androidNavigationBar": {
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.mateo_perez.go_market",
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconShop.png",
        "backgroundColor": "#231e1c"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}