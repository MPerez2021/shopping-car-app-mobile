{
  "expo": {
    "name": "Go Market",
    "slug": "go_market",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon22.png",
    "splash": {
      "image": "./assets/splashScreen1.png",
      "resizeMode": "contain",
      "backgroundColor": "#231e1c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.mateo_perez.go_market",
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconShop.png",
        "backgroundColor": "#231e1c"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
